{
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "metadata": {
                "image.type": "{image_type}",
                "image.url": "{image_url}",
                "kernel.tree": "{tree}",
                "kernel.version": "{kernel_version}",
                "device.tree": "{device_tree}",
                "kernel.endian": "{endian}",
                "kernel.defconfig": "{defconfig}",
                "platform.fastboot": "{fastboot}"
            },
            "parameters": {
                "bootloadertype": "uefi",
                "dtb": "{dtb_url}",
                "firmware": "http://images-private.armcloud.us/d01/.monitor",
                "kernel": "{kernel_url}",
                "login_prompt": "login:",
                "password": "linaro",
                "password_prompt": "Password",
                "ramdisk": "http://images.armcloud.us/misc/tyler-d01/mainline/.filesystem",
                "target_type": "ubuntu",
                "username": "root"
            }
        },
        {
            "command": "boot_linaro_image",
            "parameters": {
                "test_image_prompt": "root@d01-01:~#"
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "LAVA_SERVER",
                "stream": "BUNDLE_STREAM"
            }
        }
    ],
    "job_name": "{job_name}",
    "device_type": "{device_type}",
    "logging_level": "DEBUG",
    "timeout": 18000
}