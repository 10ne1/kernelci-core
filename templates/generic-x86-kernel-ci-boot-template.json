{
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "metadata": {
                "image.type": "{image_type}",
                "image.url": "{image_url}",
                "kernel.tree": "{tree}",
                "kernel.version": "{kernel_version}",
                "kernel.defconfig": "{defconfig}"
            },
            "parameters": {
                "bootloadertype": "ipxe",
                "kernel": "{kernel_url}",
                "ramdisk": "http://images.armcloud.us/lava/x86/ubuntu-precise-3.10-networking.gz"
            }
        },
        {
            "command": "boot_linaro_image"
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "LAVA_SERVER",
                "stream": "BUNDLE_STREAM"
            }
        }
    ],
    "device_type": "{device_type}",
    "job_name": "{job_name}",
    "logging_level": "DEBUG",
    "timeout": 18000
}